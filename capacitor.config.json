{
  "appId": "com.carol.borghesi",
  "appName": "Carol Borghesi",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "server": {
    "iosScheme": "http",
    "androidScheme": "http",
    "allowNavigation": [
      "www.googleapis.com/*",
      "*/googleapis.com"
    ],
    "hostname": "localhost"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 1000
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    }
  },
  "cordova": {}
}
